            success: function(data){
    

            document.querySelector(`#follow-${userid}` ).style.display = "initial"
            document.querySelector(`#following-${userid}`).style.display = "none"

            }
          })
        })

        })
        
        
        document.querySelectorAll(".comment").forEach(function(comment){
          let postid = comment.getAttribute("data-id")
          comment.addEventListener("click", function(){
            document.querySelector(`.overlay-${postid}`).style.display = "flex"
          })
          document.querySelector(`#close-${postid}`).addEventListener("click", function(){
            document.querySelector(`.overlay-${postid}`).style.display = "none"
          })
        })

        //comment form
        document.querySelectorAll("#submit-comment").forEach(function(comment){
            comment.addEventListener("click",function(){
                let postId = comment.getAttribute('data-id')
                let commentValue = document.querySelector(`.write-comment-${postId}`)
                $(`.comment-form-${postId}`).submit(function(event){
                    event.preventDefault()

                })
                $.ajax({
                        url: "/comment",
                        type: "POST",
                        data: {postId: postId, commentValue: commentValue.value},
                        success: function(data){
                          $(`.comments-container-${postId}`).append(`<div id="comment">
                        <div id="user-image">
                          <img onerror="this.style.display = 'none'" src="/image/<%= loggedinuser.profileImage %>" alt="">
                        </div>
                        <div id="user-dets">
                          <div id="user-name"><%= loggedinuser.username %></div>
                          <p>${commentValue.value}</p>
                        </div>
                      </div>`)
                      commentValue.value = ''
                      $(`#commentcount-${postId}`).text(data.commentArray.length)
                        }
                      
                        
                      })
            })
        })

